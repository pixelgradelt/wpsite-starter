{
  "name": "pixelgradelt/my-site",
  "type": "project",
  "license": "MIT",
  "description": "A Pixelgrade LT WordPress site.",
  "homepage": "https://local.local",
  "time": "2021-06-30T09:03:17+00:00",
  "authors": [
    {
      "name": "Vlad Olaru",
      "email": "vlad@pixelgrade.com",
      "homepage": "https://thinkwritecode.com",
      "role": "Development, infrastructure, and product development"
    }
  ],
  "keywords": [],
  "support": {
    "issues": "https://pixelgradelt.com",
    "forum": "https://pixelgradelt.com"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://lt-records.local/ltpackagist/",
      "options": {
        "ssl": {
          "verify_peer": false
        }
      }
    },
    {
      "type": "composer",
      "url": "https://repo.packagist.org"
    }
  ],
  "require": {
    "composer/installers": "^1.10",
    "ext-json": "*",
    "oscarotero/env": "^2.1",
    "php": ">=7.1",
    "pixelgradelt-records/part_another-test": "*, >=3.0.0",
    "pixelgradelt-records/part_test-test": "*, ^4.0",
    "pixelgradelt-records/part_yet-another": "*",
    "roots/bedrock-autoloader": "^1.0",
    "roots/bedrock-disallow-indexing": "^2.0",
    "roots/wordpress": "^5.7",
    "roots/wp-config": "1.0.0",
    "roots/wp-password-bcrypt": "1.0.0",
    "vlucas/phpdotenv": "^5.3"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "squizlabs/php_codesniffer": "^3.5.8"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist"
  },
  "minimum-stability": "stable",
  "prefer-stable": false,
  "extra": {
    "installer-paths": {
      "web/app/mu-plugins/{$name}/": [
        "type:wordpress-muplugin"
      ],
      "web/app/plugins/{$name}/": [
        "type:wordpress-plugin"
      ],
      "web/app/themes/{$name}/": [
        "type:wordpress-theme"
      ]
    },
    "lt-user": "",
    "lt-version": 1,
    "wordpress-install-dir": "web/wp",
    "lt-fingerprint": "a787b7bb96c4ab8929f3966039dc08b1"
  },
  "scripts": {
    "post-root-package-install": [
      "php -r \"copy('.env.example', '.env');\""
    ],
    "test": [
      "phpcs"
    ]
  }
}
